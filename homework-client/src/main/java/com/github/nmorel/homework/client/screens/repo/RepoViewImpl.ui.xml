<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder" xmlns:g="urn:import:com.google.gwt.user.client.ui"
  xmlns:c="urn:import:com.google.gwt.user.cellview.client">
  <ui:style>
    
  </ui:style>

  <ui:with type="com.github.nmorel.homework.client.resources.messages.Messages" field="mess" />
  <ui:with type="com.github.nmorel.homework.client.resources.ResourcesBundle" field="res" />

  <g:DockLayoutPanel unit='PX'>

    <g:east size="200">
      <g:ScrollPanel>
        <c:CellList ui:field='collaboratorsList' />
      </g:ScrollPanel>
    </g:east>

    <g:center>
      <g:SimplePanel ui:field='graphContainer' />
    </g:center>

  </g:DockLayoutPanel>

</ui:UiBinder>